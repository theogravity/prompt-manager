{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Library",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  }
}
